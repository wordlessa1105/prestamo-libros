export default function Biblioteca() {
  const [prestamos, setPrestamos] = useState([]);
  const [cursoSeleccionado, setCursoSeleccionado] = useState("");
  const [estudianteSeleccionado, setEstudianteSeleccionado] = useState("");
  const [letraSeleccionada, setLetraSeleccionada] = useState("");
  const [libroSeleccionado, setLibroSeleccionado] = useState("");

  useEffect(() => {
    const storedPrestamos = JSON.parse(localStorage.getItem("prestamos")) || [];
    setPrestamos(storedPrestamos);
  }, []);

  useEffect(() => {
    localStorage.setItem("prestamos", JSON.stringify(prestamos));
  }, [prestamos]);

  const prestarLibro = () => {
    if (!libroSeleccionado || !estudianteSeleccionado || !cursoSeleccionado) return;
    const fechaPrestamo = format(new Date(), "dd/MM/yyyy");
    const fechaDevolucion = format(addMonths(new Date(), 1), "dd/MM/yyyy");
    setPrestamos([
      ...prestamos,
      {
        titulo: libroSeleccionado,
        estudiante: estudianteSeleccionado,
        curso: cursoSeleccionado,
        fechaPrestamo,
        fechaDevolucion,
      },
    ]);
    setLibroSeleccionado("");
  };

  const devolverLibro = (titulo) => {
    setPrestamos(prestamos.filter((libro) => libro.titulo !== titulo));
  };

  const librosDisponibles = librosData.filter(
    (libro) =>
      (!letraSeleccionada || libro.titulo.startsWith(letraSeleccionada)) &&
      !prestamos.find((p) => p.titulo === libro.titulo)
  );

  const letras = [...new Set(librosData.map((libro) => libro.titulo[0]))].sort();

  return (
    <div className="p-6 space-y-6 max-w-6xl mx-auto bg-[#ffffff] min-h-screen">
      <div className="flex flex-col items-center space-y-4">
        <h1 className="text-3xl font-bold text-[#006ab3] text-center">
          Sistema de préstamo de libros <br /> Liceo Español Pérez Galdós
        </h1>
      </div>

      <div className="flex flex-col md:flex-row md:space-x-4 space-y-4 md:space-y-0">
        <Select onValueChange={setCursoSeleccionado} value={cursoSeleccionado}>
          <SelectTrigger className="w-full md:w-60 border-[#006ab3]">
            {cursoSeleccionado || "Selecciona un curso"}
          </SelectTrigger>
          <SelectContent>
            {Object.keys(usuariosData).map((curso) => (
              <SelectItem key={curso} value={curso}>
                {curso}
              </SelectItem>
            ))}
          </SelectContent>
        </Select>

        {cursoSeleccionado && (
          <Select onValueChange={setEstudianteSeleccionado} value={estudianteSeleccionado}>
            <SelectTrigger className="w-full md:w-60 border-[#006ab3]">
              {estudianteSeleccionado || "Selecciona un estudiante"}
            </SelectTrigger>
            <SelectContent>
              {usuariosData[cursoSeleccionado].map((usuario) => (
                <SelectItem key={usuario} value={usuario}>
                  {usuario}
                </SelectItem>
              ))}
            </SelectContent>
          </Select>
        )}

        {estudianteSeleccionado && (
          <>
            <Select onValueChange={setLetraSeleccionada} value={letraSeleccionada}>
              <SelectTrigger className="w-full md:w-60 border-[#006ab3]">
                {letraSeleccionada || "Filtra el libro por letra"}
              </SelectTrigger>
              <SelectContent>
                {letras.map((letra) => (
                  <SelectItem key={letra} value={letra}>
                    {letra}
                  </SelectItem>
                ))}
              </SelectContent>
            </Select>

            <Select onValueChange={setLibroSeleccionado} value={libroSeleccionado}>
              <SelectTrigger className="w-full md:w-60 border-[#006ab3]">
                {libroSeleccionado || "Selecciona un libro"}
              </SelectTrigger>
              <SelectContent>
                {librosDisponibles.map((libro) => (
                  <SelectItem key={libro.titulo} value={libro.titulo}>
                    {libro.titulo}
                  </SelectItem>
                ))}
              </SelectContent>
            </Select>

            <Button onClick={prestarLibro} className="w-full md:w-60 bg-[#006ab3] hover:bg-[#004f87]">
              Prestar libro
            </Button>
          </>
        )}
      </div>

      <h2 className="text-2xl font-semibold mt-8 text-center text-[#e20031]">Libros Prestados</h2>
      <div className="grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 gap-4">
        {prestamos.map((libro, index) => (
          <Card key={index} className="bg-[#d7ae00] hover:shadow-xl transition text-white">
            <CardContent className="p-6 space-y-3">
              <div className="text-lg font-bold">{libro.titulo}</div>
              <div className="text-sm">Estudiante: <span className="font-medium">{libro.estudiante}</span></div>
              <div className="text-sm">Curso: <span className="font-medium">{libro.curso}</span></div>
              <div className="text-xs">Prestado: {libro.fechaPrestamo}</div>
              <div className="text-xs">Devolver antes de: {libro.fechaDevolucion}</div>
              <Button
                onClick={() => devolverLibro(libro.titulo)}
                variant="destructive"
                size="sm"
                className="w-full bg-[#e20031] hover:bg-[#b10027]"
              >
                Devolver libro
              </Button>
            </CardContent>
          </Card>
        ))}
      </div>
    </div>
  );
}
